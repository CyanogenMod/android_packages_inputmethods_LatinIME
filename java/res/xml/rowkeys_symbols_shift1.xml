<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin"
>
    <Key
        latin:keyLabel="~" 
	latin:moreKeys="®,©" />
    <Key
        latin:keyLabel="`" />
    <Key
        latin:keyLabel="|" />
    <!-- U+221A: "√" SQUARE ROOT -->
    <Key
        latin:keyLabel="&#x221A;" />
    <Key 
	latin:keyLabel="π" 
	latin:moreKeys="γ,δ,β,ε,α,θ,ι,η,κ,ζ,ν,ξ,μ,o,λ,ς,τ,σ,υ,ρ,,ψ,ω,χ,φ" />
    <Key 
	latin:keyLabel="Δ" 
	latin:moreKeys="Γ,Ε,Β,Ζ,Α,Ι,Κ,Θ,Λ,Η,Ξ,Ο,Ν,Π,Μ,Τ,Υ,Σ,Φ,Ρ,Χ,Ψ" />
    <!-- U+00F7: "÷" DIVISION SIGN -->
    <Key
        latin:keyLabel="&#x00F7;" />
    <!-- U+00D7: "×" MULTIPLICATION SIGN -->
    <Key
        latin:keyLabel="&#x00D7;" />
    <include
        latin:keyboardLayout="@xml/keys_curly_brackets" />
</merge>
