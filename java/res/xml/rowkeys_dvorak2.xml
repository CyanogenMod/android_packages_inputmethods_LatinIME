<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin"
>
    <Key
        latin:keySpec="a"
        latin:keyHintLabel="\\"
        latin:additionalMoreKeys="\\" />
    <Key
        latin:keySpec="o"
        latin:keyHintLabel="`"
        latin:additionalMoreKeys="`" />
    <Key
        latin:keySpec="e"
        latin:keyHintLabel="~"
        latin:additionalMoreKeys="~" />
    <Key
        latin:keySpec="u"
        latin:keyHintLabel="|"
        latin:additionalMoreKeys="|" />
    <Key
        latin:keySpec="i"
        latin:keyHintLabel="["
        latin:additionalMoreKeys="[" />
    <Key
        latin:keySpec="d"
        latin:keyHintLabel="]"
        latin:additionalMoreKeys="]" />
    <Key
        latin:keySpec="h"
        latin:keyHintLabel="&lt;"
        latin:additionalMoreKeys="&lt;" />
    <Key
        latin:keySpec="t"
        latin:keyHintLabel="&gt;"
        latin:additionalMoreKeys="&gt;" />
    <Key
        latin:keySpec="n"
        latin:keyHintLabel="!text/keyspec_left_curly_bracket"
        latin:additionalMoreKeys="!text/keyspec_left_curly_bracket" />
    <Key
        latin:keySpec="s"
        latin:keyHintLabel="!text/keyspec_right_curly_bracket"
        latin:additionalMoreKeys="!text/keyspec_right_curly_bracket" />
</merge>
