<Keyboard
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="@dimen/key_bottom_gap"
    android:keyHeight="@dimen/key_height"
>
    <Row
        android:rowEdgeFlags="top"
    >
        <Key
            android:keyLabel="ק"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="!"
            android:keyEdgeFlags="left" />
        <Key
            android:keyLabel="ר"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="&quot;" />
        <Key
            android:keyLabel="א"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="'" />
        <Key
            android:keyLabel="ט"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=":" />
        <Key
            android:keyLabel="ו"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=";" />
        <Key
            android:keyLabel="ן" 
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="/" />
        <Key
            android:keyLabel="ם"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="?" />
        <Key
            android:keyLabel="פ"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="=" />
        <Key
            android:codes="@integer/key_delete"
            android:keyIcon="@drawable/sym_bkeyboard_delete"
            android:iconPreview="@drawable/sym_keyboard_feedback_delete"
            android:keyWidth="15%p"
            android:isRepeatable="true"
            android:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key
            android:keyLabel="ש"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="0"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left" />
        <Key
            android:keyLabel="ד"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="1" />
        <Key
            android:keyLabel="ג"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="2" />
        <Key
            android:keyLabel="כ"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="3" />
        <Key
            android:keyLabel="ע"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="4" />
        <Key
            android:keyLabel="י"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="5" />
        <Key
            android:keyLabel="ח"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="6" />
        <Key
            android:keyLabel="ל"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="7" />
        <Key
            android:keyLabel="ך"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="8" />
        <Key
            android:keyLabel="ף"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="9"
            android:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key
            android:keyLabel="ז"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="\@"
            android:keyEdgeFlags="left" />
        <Key
            android:keyLabel="ס"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="#" />
        <Key
            android:keyLabel="ב"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="$" />
        <Key
            android:keyLabel="ה"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="&amp;" />
        <Key
            android:keyLabel="נ"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="*" />
        <Key
            android:keyLabel="מ"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="-" />
        <Key
            android:keyLabel="צ"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="+" />
        <Key
            android:keyLabel="ת"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="(" />
        <Key
            android:keyLabel="ץ"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=")"
            android:keyEdgeFlags="right" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_normal"
        android:keyWidth="10%p"
        android:rowEdgeFlags="bottom"
    >
        <Key
            android:codes="@integer/key_symbol"
            android:keyLabel="@string/label_symbol_key"
            android:keyWidth="20%p"
            android:keyEdgeFlags="left" />
        <Key
            android:codes="@integer/key_f1" />
        <Key
            android:codes="@integer/key_space"
            android:keyIcon="@drawable/sym_bkeyboard_space"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:keyWidth="40%p" />
        <Key
            android:keyLabel="."
            android:keyIcon="@drawable/hint_popup"
            android:popupKeyboard="@xml/popup_punctuation" />
        <Key
            android:codes="@integer/key_return"
            android:keyIcon="@drawable/sym_bkeyboard_return"
            android:iconPreview="@drawable/sym_keyboard_feedback_return"
            android:keyWidth="20%p"
            android:keyEdgeFlags="right" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_url"
        android:keyWidth="10%p"
        android:rowEdgeFlags="bottom"
    >
        <Key
            android:codes="@integer/key_symbol"
            android:keyLabel="@string/label_symbol_key"
            android:keyWidth="20%p"
            android:keyEdgeFlags="left" />
        <Key
            android:keyLabel="/" />
        <Key
            android:codes="@integer/key_space"
            android:keyIcon="@drawable/sym_bkeyboard_space"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:keyWidth="40%p" />
        <Key
            android:keyLabel="."
            android:keyIcon="@drawable/hint_popup"
            android:popupKeyboard="@xml/popup_punctuation" />
        <Key
