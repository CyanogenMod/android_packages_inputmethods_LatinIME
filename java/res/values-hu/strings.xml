    <!-- Option to provide vibrate/haptic feedback on keypress -->
    <string name="vibrate_on_keypress">Rezgés gombnyomáskor</string>
    <!-- Option to play back sound on keypress in soft keyboard -->
    <string name="sound_on_keypress">Hang gombnyomáskor</string>
    
    <!-- Option to enable using nearby keys when correcting/predicting -->
    <string name="hit_correction">Gépelési hibák javítása</string>
    
    <!-- Description for hit_correction  -->
    <string name="hit_correction_summary">Hibajavítás engedélyezése</string>
    
