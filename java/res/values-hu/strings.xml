    <string name="hit_correction_land">Hibajavítás fekvő nézetben</string>
    
    <!-- Description for hit_correction in landscape -->
	<string name="hit_correction_land_summary">Gépelési hibák javítása</string>
    
    <!-- Option to automatically correct word on hitting space -->
	<string name="auto_correction">Automatikus javítás</string> 
    
    <!-- Description for auto_correction -->
	<string name="auto_correction_summary">Az előző szó automatikus javítása a szóköz leütésekor</string>
	
    <!-- Option to enable text prediction -->
