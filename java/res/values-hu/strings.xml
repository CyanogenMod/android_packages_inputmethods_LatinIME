    <string name="auto_complete_dialog_title">Automatikus kiegészítés</string>
    
    <!-- Option to enable text prediction in landscape -->
    <string name="prediction_landscape">Szövegmező méretének növelése</string> 
    <!-- Description for text prediction -->
    <string name="prediction_landscape_summary">Szójavaslatok elrejtése fektetett nézetben</string>
	
    <!-- Option to enable auto capitalization of sentences -->
    <string name="auto_cap">Automatikus nagybetű</string> 
    <!-- Description for auto cap -->
    <string name="auto_cap_summary">Automatikus nagybetű a mondatok elején</string>
    <!-- Option to enable auto punctuate -->
    <string name="auto_punctuate">Automatikus írásjelek</string> 
    <!-- Description for auto punctuate -->
    <string name="auto_punctuate_summary"/>
    
    <!-- Option to enable quick fixes -->
    <string name="quick_fixes">Gyors javítások</string>
    <!-- Description for quick fixes -->
    <string name="quick_fixes_summary">Gyakori gépelési hibák javítása</string>
    
    <!-- Option to enable showing suggestions -->
    <string name="show_suggestions">Javaslatok megjelenítése</string>
    <!-- Description for show suggestions -->
    <string name="show_suggestions_summary">Javasolt szavak kijelzése gépelés közben</string>
    
    <!-- Option to enable auto completion -->
    <string name="auto_complete">Automatikus kiegészítés</string>
    <!-- Description for auto completion -->
    <string name="auto_complete_summary">Kiemelt szójavaslat esetén automatikus szóköz- és írásjel-beillesztés</string>
    
